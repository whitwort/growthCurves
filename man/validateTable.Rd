## The function is currently defined as
function (table) 
{
    badRows <- apply(table, 1, function(r) {
        NA \%in\% r
    })
    if (TRUE \%in\% badRows) {
        print("The following timepoints will be purged because they contain incomplete data:")
        print(paste(table$time[badRows], sep = ","))
        table <- table[!badRows, ]
    }
    return(table)
  }
}
